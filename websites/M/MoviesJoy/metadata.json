{
	"$schema": "https://schemas.premid.app/metadata/1.9",
	"author": {
		"id": "261744277154037762",
		"name": "atermelon"
	},
	"service": "SoapTV",
	"description": {
		"en": "You can watch all videos without logging in or registration"
	},
	"url": "Soaper.tV","soaper.tv","Soaper.live",
	"regExp": "(www[.])?(ww[0-9]{0,2}[.])?soaperTV[.].*",
	"version": "15.0.0",
	"logo": "https://soaper.tv/logo.png",
	"thumbnail": "https://i.imgur.com/nD7ZB3v.jpeg",
	"color": "#ffffff",
	"category": "videos",
	"tags": [
		"movies",
		"series",
		"watch",
		"free"
	],
	"iframe": true,
	"iFrameRegExp": ".*",
	"settings": [
		{
			"id": "privacy",
			"title": "Privacy Mode",
			"icon": "fas fa-user-secret",
			"value": false
		},
		{
			"id": "buttons",
			"title": "Show Buttons",
			"icon": "fas fa-compress-arrows-alt",
			"value": true,
			"if": {
				"privacy": false
			}
		},
		{
			"id": "covers",
			"title": "Show Covers",
			"icon": "fas fa-images",
			"value": true,
			"if": {
				"privacy": false
			}
		}
	]
}
